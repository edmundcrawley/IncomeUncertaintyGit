@techreport{basu_uncertainty_2011,
	type = {Boston {College} {Working} {Papers} in {Economics}},
	title = {Uncertainty {Shocks} in a {Model} of {Effective} {Demand}},
	url = {https://ideas.repec.org/p/boc/bocoec/774.html},
	abstract = {Can increased uncertainty about the future cause a contraction in output and its components? An identified uncertainty shock in the data causes significant declines in output, consumption, investment, and hours worked. Standard general-equilibrium models with flexible prices cannot reproduce this comovement. However, uncertainty shocks can easily generate comovement with countercyclical markups through sticky prices. Monetary policy plays a key role in offsetting the negative impact of uncertainty shocks during normal times. Higher uncertainty has even more negative effects if monetary policy can no longer perform its usual stabilizing function because of the zero lower bound. We calibrate our uncertainty shock process using fluctuations in implied stock market volatility and show that the model with nominal price rigidity is consistent with empirical evidence from a structural vector autoregression. We argue that increased uncertainty about the future likely played a role in worsening the Great Recession.},
	number = {774},
	institution = {Boston College Department of Economics},
	author = {Basu, Susanto and Bundick, Brent},
	year = {2011},
	keywords = {Monetary Policy, Sticky-Price Models, Uncertainty Shocks, Zero Lower Bound on Nominal Interest Rates},
	file = {RePEc PDF:C\:\\Users\\edmun\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ni0zxbcx.default\\zotero\\storage\\KEKP6IGQ\\Basu and Bundick - 2011 - Uncertainty Shocks in a Model of Effective Demand.pdf:application/pdf;RePEc Snapshot:C\:\\Users\\edmun\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ni0zxbcx.default\\zotero\\storage\\I7D78SMG\\774.html:text/html}
}

@article{leduc_uncertainty_2016,
